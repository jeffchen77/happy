}

.mune {
	float: left;
	margin-left: 0.54rem;
	width: 1.2rem;
	height: 0.7rem;
	text-align: center;
	color: #838383;
}

.mune p {
	font-size: 0.22rem;
	color: #838383;
}


 
.edit-button {
	display: block;
	position: absolute;
	top: 0.4rem;
	right: 0.25rem;
	width: 0.32rem;
	height: 0.35rem;
	background: url(../../images/editIcon.png) no-repeat;
	background-size: 0.32rem 0.35rem;
}
.DetaPageTopBtn {
    width: 100%;
    max-width: 640px;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    margin: auto;
    display: table;
    background: #5bc0de;
}
.DetaPageTopBtn a {
    width: 50%;
    height: 44px;
    display: table-cell;
    font: normal 14px/44px 'Microsoft Yahei';
    text-align: center;
    color: white;
    position: relative;
}
.CarePageTopBtn {
    width: 100%;
