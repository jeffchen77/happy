.tag {
	width: auto;
	height: auto;
	position: relative;
	background-color: rgba(153, 153, 153, 0.33);
	margin-top: 0.5rem;
	padding: 0.2rem 0.3rem 0.1rem;
}

.tag ul{
padding: 0px 0px 0px 20px;	
}

.tag ul li{
	list-style-type: none;
	
}

.tag:before, .tag:after {
	content: "";
	display: block;
	border-width: 10px;
	position: absolute;
	top: -20px;
	border-style: solid dashed dashed;
	border-color: transparent transparent rgba(153, 153, 153, 0.33)
		transparent;
	font-size: 0;
	line-height: 0;
}

.tag:after {
	border-color: transparent transparent rgba(153, 153, 153, 0) transparent;
}



