.quantity-wrapper {
    display: inline-block;
    height: 24px;
    border-radius: 5px;
    margin-left: 8px;
}
.quantity-decrease, .quantity, .quantity-increase {
    float: left;
    text-align: center;
    width: 0.8rem;
    height: 100%;
    border: 1px solid #232326;
    border-left: none;
    border-right: none;
}
.quantity-decrease, .quantity-increase {
    background: #fff;
    border: 1px solid #232326;
    color: #232326;
    display: block;
    line-height: 24px;
    width: 24px;
    overflow: hidden;
    text-indent: -200px;
}
.quantity-decrease {
    -webkit-border-radius: 2px 0 0 2px;
    background-position: 10px -20px;
}
.quantity-increase {
    -webkit-border-radius: 0 2px 2px 0;
}
.quantity-decrease em {
    background: url(../images/cart-number.png) no-repeat 0 -18px;
    background-size: 100%;
    height: 10px;
    width: 10px;
    display: block;
    margin: 7px;
}
.quantity-increase em {
    background: url("../images/cart-number.png") no-repeat 0 0;
    background-size: 100%;
    height: 10px;
    width: 10px;
    display: block;
    margin: 7px;
}
