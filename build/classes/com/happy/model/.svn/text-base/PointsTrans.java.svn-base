package com.happy.model;

import java.sql.Timestamp;

import org.nutz.dao.entity.annotation.Column;
import org.nutz.dao.entity.annotation.Table;

@Table("points_trans")
public class PointsTrans {
	@Column("id")
	private int id;
	
	@Column("act_pgm_id")
	private String actPgmId;
	
	@Column("user_id")
	private String userId;
	
	@Column("bef_trans_points")
	private int befTransPoints;
	
	@Column("trans_points")
	private int transPoints;
	
	@Column("aft_trans_points")
	private int aftTransPoints;
	
	@Column("trans_type")
	private int transType;
	
	@Column("trans_time")
	private Timestamp transTime;

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getActPgmId() {
		return actPgmId;
	}

	public void setActPgmId(String actPgmId) {
		this.actPgmId = actPgmId;
	}

	public String getUserId() {
		return userId;
	}

	public void setUserId(String userId) {
		this.userId = userId;
	}

	public int getBefTransPoints() {
		return befTransPoints;
	}

	public void setBefTransPoints(int befTransPoints) {
		this.befTransPoints = befTransPoints;
	}

	public int getTransPoints() {
		return transPoints;
	}

	public void setTransPoints(int transPoints) {
		this.transPoints = transPoints;
	}

	public int getAftTransPoints() {
		return aftTransPoints;
	}

	public void setAftTransPoints(int aftTransPoints) {
		this.aftTransPoints = aftTransPoints;
	}

	public int getTransType() {
		return transType;
	}

	public void setTransType(int transType) {
		this.transType = transType;
	}

	public Timestamp getTransTime() {
		return transTime;
	}

	public void setTransTime(Timestamp transTime) {
		this.transTime = transTime;
	}
	
}
